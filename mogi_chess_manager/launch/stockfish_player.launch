<?xml version="1.0"?>

<launch>

  <arg name="side" default="b"/>
  <arg name="type" default="stockfish"/>

  <node name="$(anon mogi_chess_stockfish)" pkg="mogi_chess_manager" type="player.py" output ="screen">
    <rosparam subst_value="true">
      side: $(arg side)
      type: $(arg type)
    </rosparam>
  </node>

</launch>
